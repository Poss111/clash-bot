  "dependencies": {
    "@fightmegg/riot-api": "0.0.7",
    "aws-sdk": "^2.898.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "dynamodb": "^1.3.0",
    "joi": "^17.4.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "jest-each": "^27.0.6",
    "nock": "^13.0.11",
    "nodemon": "^2.0.8",
    "stream-mock": "^2.0.5",
    "streamtest": "^2.0.0"
  }
