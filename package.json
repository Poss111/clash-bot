{
  "name": "clash-bot",
  "version": "1.0.0",
  "description": "A Discord bot for League of Legends Clash team management.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest unit --coverage --coverageReporters=\"json\" --coverageReporters=\"json-summary\" --coverageReporters=\"lcov\" --coverageReporters=\"text\"",
    "preintegration": "docker-compose up --no-start --force-recreate dynamodb-local && docker-compose start",
    "integration": "jest int",
    "postintegration": "docker-compose stop"
  },
