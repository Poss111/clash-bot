{
  "name": "clash-bot",
  "version": "1.0.0",
  "description": "A Discord bot for League of Legends Clash team management.",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/nodemon index.js",
    "local-start": "./node_modules/.bin/nodemon index.js | pino-pretty -t",
