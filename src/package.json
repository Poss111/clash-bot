    "cors": "^2.8.5",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment-timezone": "^0.5.33",
    "pino": "^7.11.0",
    "query-string": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-plugin-jest": "^26.8.2",
