    "integration": "./node_modules/.bin/jest int --silent",
    "lint": "eslint '**/*.js'  --ignore-pattern node_modules/  --ignore-pattern commands/tests/test-data"
  },
  "keywords": [],
  "author": "Daniel Poss",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
